{
  "name": "phala-js-sdk",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:sdk": "yarn workspace @phala/sdk build",
    "dev": "concurrently \"yarn dev:sdk\" \"yarn dev:example\"",
    "dev:example": "yarn workspace example dev",
    "dev:sdk": "yarn workspace @phala/sdk dev",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "concurrently": "^6.2.2",
    "jest": "^27.2.2",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
